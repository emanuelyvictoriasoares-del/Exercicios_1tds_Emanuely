#include <stdio.h>

int main(){int n;
printf("Digite um número:");
scanf("%d", &n);
printf("O drobo é %d", n*2);

return 0;
   
}



CERTO//////////////////////////////////
#include <stdio.h>

int numero; //variavel para guardar o número digitado

int main()
{
    printf("Digite um número:"); //pede para digitar o número
    scanf("%d", &numero); // lê o valor digitado
    
    //calcula e mostra o dobro do número
    printf("O dobro é: %d\n", numero * 2);
    
    return 0;
    
}
